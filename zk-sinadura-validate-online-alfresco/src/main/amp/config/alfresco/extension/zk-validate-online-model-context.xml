<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>

	<!-- Registration of new models -->
	<bean id="extension.dictionaryBootstrapZKValidate" parent="dictionaryModelBootstrap"
		depends-on="dictionaryBootstrap">
		<property name="models">
			<list>
				<value>alfresco/extension/model/zk-validate-online-model.xml</value>
			</list>
		</property>
		<property name="labels">
			<list>
				<value>alfresco/extension/model/zk-validate-online-model</value>
			</list>
		</property>
	</bean>


	<!-- Webscripts -->
	<bean id="webscript.net.zylk.zk-validate-signed-content.post"
		  class="net.zylk.validate.online.webscripts.ValidateSignedContentWebscript" parent="webscript">
		<property name="registry" ref="ServiceRegistry" />
	</bean>


	<!-- Action Executers -->
	<bean id="validate-signed-content"
		  class="net.zylk.validate.online.action.executer.ValidateSignedContentActionExecuter" parent="action-executer">
		<property name="registry" ref="ServiceRegistry" />
	</bean>

</beans>

